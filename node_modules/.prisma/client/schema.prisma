datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum MeasureType {
  WATER
  GAS
}

model LeituraResposta {
  measure_uuid     String      @id @default(uuid())
  customer_code    String      @unique
  measure_datetime DateTime
  measure_type     MeasureType
  imageURL         String
  confirmed_value  Int?

  @@map("leituraResposta")
}
